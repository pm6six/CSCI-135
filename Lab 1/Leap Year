#include <iostream>
using namespace std;
  int main()
{
  int year;
  cout << "Enter year: ";
  cin >> year;
  
if (year % 4 != 0)
  cout << "Common year" << endl;
  // is not divisible by 4) then (it is a common year) 
else if (year % 100 != 0)
  cout << "Leap year" << endl;
  // is not divisible by 100) then (it is a leap year) 
else if (year % 400 != 0)
  cout << "Common year" << endl;
  //is not divisible by 400) then (it is a common year) 
else 
  cout << "Leap year" << endl;  
  // (it is a leap year)
  
  return 0;
}
